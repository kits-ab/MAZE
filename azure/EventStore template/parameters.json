{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "containerName": {
            "value": "maze-events"
        },
        "location": {
            "value": "northeurope"
        },
        "imageType": {
            "value": "Public"
        },
        "imageName": {
            "value": "eventstore/eventstore"
        },
        "osType": {
            "value": "Linux"
        },
        "numberCpuCores": {
            "value": "1"
        },
        "memory": {
            "value": "1.5"
        },
        "restartPolicy": {
            "value": "OnFailure"
        },
        "environmentVariables": {
            "value": [
                {
                    "name": "EVENTSTORE_DEV",
                    "value": "True"
                },
                {
                    "name": "EVENTSTORE_ENABLE_EXTERNAL_TCP",
                    "value": "True"
                }
            ]
        },
        "ipAddressType": {
            "value": "Public"
        },
        "ports": {
            "value": [
                {
                    "port": "80",
                    "protocol": "TCP"
                },
                {
                    "port": "1113",
                    "protocol": "TCP"
                },
                {
                    "port": "2113",
                    "protocol": "TCP"
                }
            ]
        },
        "dnsNameLabel": {
            "value": "maze-events"
        }
    }
}